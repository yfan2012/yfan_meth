#!/bin/bash -l

#SBATCH --account=mschatz1
#SBATCH --time=0:30:0
#SBATCH --partition=shared
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=1
#SBATCH --cpus-per-task=22

i=$SLURM_ARRAY_TASK_ID

if [ -d "$1/called/${i}/" ]; then 
    echo "Already called this one: ${i}"
else

    echo "calling ${i}"
    read_fast5_basecaller.py -v
    read_fast5_basecaller.py -i $1/raw/${i}/ -t 22 -s $1/called/${i}/ --flowcell FLO-MIN106 --kit SQK-LSK108 -o fastq,fast5

fi



